<body>

<!--HEADER FOR SINGLE COLUMN PAGE--->
<div id = "head" class="col-lg-12">
  <header>
    <nav class="navbar"> <a class="navbar-brand" href="https://www.lps.org/"> <img src="../resources/Small LPS Logo.png" width = "39" height = "30" class="d-inline-block align-top" alt=""> LPS Gallery </a>
        
        
      <cfset filePath = "GetCurrentTemplatePath()">
      </cfset>
    <cfoutput>#filePath#</cfoutput>
      <cfset fileName = "GetFileInfo(filepath).name">
      </cfset>
      
      <!--CONIDITIONAL START-->
      <cfif fileName = "index">
        <!--SEARCH-->
        <form class="form-inline my-2 my-lg-0">
          <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
          <button class="btn btn-light my-2 my-sm-0" id = "submit-btn" type="submit">Search</button>
        </form>
        <cfelse>
        <!--NON SEARCH-->
        <button type="button" class="btn btn-outline-light" id = "back-btn">BACK TO GALLERY</button>
        </cfelse>
      </cfif>
      <!---END OF CONIDITIONAL---> 
      
    </nav>
  </header>
</div>
<!--END OF HEADER FOR SINGLE COLUMN PAGE--> 

<!--START OF SINGLE COLUMN CONTENT--->
<div id="content-single" class="col-lg-12"> <!-- TemplateBeginEditable name="page-content" --><!-- TemplateEndEditable --> </div>
